{
	"compilerOptions": {
		"outDir": "./dist/",
		"sourceMap": true,
		"module": "commonjs",
		"target": "es5",
		"jsx": "react",
		"allowSyntheticDefaultImports": true,
		"noImplicitAny": true,
		"removeComments": false,
		"preserveConstEnums": true
	},
	"exclude": [
		"node_modules"
	],
	"include": [
		"./src/**/*"
	]
}